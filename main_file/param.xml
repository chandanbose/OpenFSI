<?xml version="1.0" ?>

<geometry>
    <Resolution> 1 </Resolution>
    <!-- One lattice has N cells -->
    <Viscosity> 0.16667 </Viscosity>
    <!-- Reynolds number -->
    <x_length> 60 </x_length>
    <y_length> 60 </y_length>
	<z_length> 60 </z_length>
    <!-- size of the flow domain -->
</geometry>

<!-- All fluid properties are given in physical units. -->
<fluid>
    <Shear_flag> 1 </Shear_flag>
	<!-- shear flow flag -->
    <Shear_Top_Velocity> 0.1 </Shear_Top_Velocity>
    <Shear_Bot_Velocity> -0.1 </Shear_Bot_Velocity>
	<Poiseuille_flag> 0 </Poiseuille_flag>
	<!-- Poiseuille flow flag -->
	<Poiseuille_bodyforce> 0.0001 </Poiseuille_bodyforce>
</fluid>

<simulation>
    
    <Total_timestep>  20000 </Total_timestep>
    <!-- Maximum timestep for simulation. -->
    <Output_fluid_file> 2000      </Output_fluid_file>
    <Output_check_file>  2000  </Output_check_file>

    <CouplingType>      1   </CouplingType>
    <!-- CouplingType: 1: velocity coupling(default); 2: force coupling -->
</simulation>

